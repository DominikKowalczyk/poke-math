{
    "manifest_version": 3,
    "name": "Poke Math",
    "version": "1.0",
    "description": "Replaces new tab with a math question.",
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    },
    "web_accessible_resources": [
      {
        "resources": ["katex.min.js", "katex.min.css", "fonts/*"],
        "matches": ["<all_urls>"]
      }
    ]
  }
  