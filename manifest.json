{
  "manifest_version": 3,
  "name": "Pierre",
  "version": "1.0",
  "description": "Replaces new tab with a math question.",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["katex.min.js", "katex.min.css", "fonts/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "icons/favicon-16x16.png",
    "48": "icons/favicon-48x48.png",
    "128": "icons/favicon-128x128.png"
  }
}